{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [ 
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    
    "rules": { 
        "prefer-const": "warn",
        "eqeqeq": "warn"
    },
    "globals": {
        "$": true,
        "require": true,
        "expect": true,
        "test": true,
        "module":true
    }
}